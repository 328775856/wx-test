@import '../../../../utils/iconfont.wxss';
.srcoll_view{
  height: 100px;
  white-space: nowrap;
}
.flex-item{
  width: 200px;
  height: 100%;
  box-sizing: border-box;
  display: inline-block;
}

.bc_green{
  background-color: green;
}

.bc_red{
  background-color: red;
}

.bc_blue{
  background-color: blue;
}
.bc_yellow{
  background-color: yellow;
}

.clickItem{
  margin-top: 20px;
  background-color: grey;
  height: 40px;
  border-radius: 5px;
  line-height: 40px;
}
.bd{
  border: 1px solid #000;
  white-space: inherit;
}
.searchbd{
  border: 1px solid #777;
  white-space: inherit;
  border-radius: 5px;
  margin: 4px;
}

.section{
  height: 300px;
  text-align: center;
}
.item{
  width: 48%;
  height: 54%;
  display: inline-block;
  margin: 2px;
}
.item image{
  width: 100%;
  height: 100%;
  
  padding: 0;
}
.footer{
  background: #333;
  height: 100px;
  width: 100%;
}
.item1{
  width: 33%;
  height: 100px;
  border: 0px solid #000;
  display: inline-block;
  text-align: center;
  line-height: 100px;
  color: #fff;
  font-size: 15px;
}



